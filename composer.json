{
    "name": "mbezhanov/silex-route-annotations",
    "description": "A service provider that allows you to use @Route annotations in your Silex applications.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marin Bezhanov",
            "email": "marin.bezhanov@gmail.com",
            "homepage": "http://marinbezhanov.com"
        }
    ],
    "autoload": {
        "psr-4": { "Bezhanov\\Silex\\Routing\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Bezhanov\\Silex\\Routing\\Tests\\": "tests" }
    },
    "require": {
        "php": "^7.0",
        "silex/silex": "^2.1",
        "symfony/cache": "^3.3",
        "symfony/config": "^3.3",
        "doctrine/annotations": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    }
}
